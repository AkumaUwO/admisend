<!-- HTML -->
<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Panel de Administrador</title>
    <link rel="stylesheet" href="stylesheets/style_admin.css">
</head>
<body>
    <!-- Navbar -->
    <nav class="navbar">
        <div class="logo">
            <img src="logo.png" alt="Logo de la aplicación">
        </div>
        <div class="nav-links">
            <a href="#" class="btn-logout">Cerrar sesión</a>
        </div>
    </nav>

    <!-- Sidebar -->
    <aside class="sidebar">
        <ul>
            <li><a href="#" class="active">Usuarios</a></li>
            <li><a href="#">Mensajes</a></li>
            <li><a href="#">Departamentos</a></li>
        </ul>
    </aside>

    <!-- Contenido -->
    <main class="content">
        <h1>Usuarios</h1>
        <table>
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Nombre</th>
                    <th>Apellido</th>
                    <th>Email</th>
                    <th>Acciones</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>1</td>
                    <td>John</td>
                    <td>Doe</td>
                    <td>johndoe@example.com</td>
                    <td>
                        <a href="#" class="btn-edit">Editar</a>
                        <a href="#" class="btn-delete">Eliminar</a>
                    </td>
                </tr>
                <tr>
                    <td>2</td>
                    <td>Jane</td>
                    <td>Doe</td>
                    <td>janedoe@example.com</td>
                    <td>
                        <a href="#" class="btn-edit">Editar</a>
                        <a href="#" class="btn-delete">Eliminar</a>
                    </td>
                </tr>
                <!-- Más filas de datos aquí -->
            </tbody>
        </table>
    </main>
</body>
</html>